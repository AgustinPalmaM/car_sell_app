<div class="car">
  <p><%= car.registration %></p>
  <p><%= car.brand %></p>
  <p><%= car.version %></p>
  <p><%= car.model %></p>
  <p><%= car.price %></p>
  <div>
    <%= image_tag(car.photo, width: 200) if car.photo.attached? %>
  </div>
  <%= link_to  "show more", car_path(car) %>
  <%= link_to  "delete", car_path(car), data: { turbo_method: :delete, turbo_confirm: t('common.confirm') } %>
</div>
